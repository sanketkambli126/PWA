@model PurchaseOrderRequestViewModel
@{
    Layout = "~/Views/Shared/base.cshtml";
}

@section Title
{
    Procurement - @Model.VesselName
}

@section ContentSection
{
    <div class="app-main__outer fixed-page-header">

        @Html.HiddenFor(m => m.FromDate)
        @Html.HiddenFor(m => m.ToDate)
        @Html.HiddenFor(m => m.VesselId)
        @Html.HiddenFor(m => m.AccountCompanyId)
        @Html.HiddenFor(m => m.POStage)
        @Html.HiddenFor(m => m.PurchaseOrderTypes)
        @Html.HiddenFor(m => m.SearchOrderNumber)
        @Html.HiddenFor(m => m.SelectedOrderStage)
        @Html.HiddenFor(m => m.IsSearchClicked)
        @Html.HiddenFor(m => m.Title)
        @Html.HiddenFor(m => m.SupplierId)
        @Html.HiddenFor(m => m.SupplierName)
        @Html.HiddenFor(m => m.SelectedStatus)
        @Html.HiddenFor(m => m.ActiveMobileTabClass)
        @Html.HiddenFor(m => m.ShowOnlyAuthRequired)
        @Html.HiddenFor(m => m.strOrderStatusIds)

        <div class="app-main__inner background-padding new-background common-border-radius mobile-tab-bottom-padding polistdesign">
            <div class="app-page-title header-shadow">
                <div class="page-title-wrapper">
                    <div class="margin-left-35">
                        <div>
                            <a class="common-heading-green"
                               href="@string.Concat(@Constants.DashboardVesselNavigation,@Model.VesselId)">@Model.VesselName</a>
                        </div>

                        <div class="common-sub-heading-black">
                            Procurement
                        </div>
                    </div>
                    <div class="page-title-actions">
                        <button class="btn btn-dark-grey text-white btn-shadow hover-blue mr-1 search-filter-sidebar d-md-inline-block d-none" id="divfilterhide">
                            <i class="fa fa-filter" aria-hidden="true" title=""><span class="open-sans-font bold-text ml-2 d-none d-md-inline-block">Filter</span></i>
                        </button>
                        <div class="d-md-inline-block d-none mr-1 mb-mobile-3 datepicker-mobile-design" id="divDatePickermobilehide">
                            <button class="btn btn btn-shadow btn-info date-picker-text-bold iconsdatepicker" id="dtrOrderlist" name="datefilter">
                            </button>
                        </div>
                        <button id="btnExport" data-toggle="tooltip" title="Export to excel" data-placement="bottom" class="btn btn-dark-grey text-white btn-export btn-shadow hover-blue mobile-fixed-right">
                            <i class="fa fa-fw" aria-hidden="true"></i>
                        </button>
