                            {
                                <span class="badge badge-pill badge-danger purchase-order-status-badge" data-toggle="tooltip" data-placement="bottom" title="@Model.OrderStatusName">
                                    @Model.OrderStatus
                                </span>
                            }
                            else
                            {
                                <span class="badge badge-pill badge-success purchase-order-status-badge" data-toggle="tooltip" data-placement="bottom" title="@Model.OrderStatusName">
                                    @Model.OrderStatus
                                </span>
                            }
                            <span class="header">
                                @Model.AccountingCompanyId - @Model.OrderNumber
                                @Model.Title
                            </span>
                            <div class="">
                                @if (Model.IsFromViewRecord == true)
                                {
                                    <span class="common-small-heading">
                                        @Model.VesselName
                                    </span>
                                }
                                else
                                {
                                    <a class="common-heading-green"
                                       href="@string.Concat(@Constants.DashboardVesselNavigation,@Model.PurchaseOrderVesselId)">@Model.VesselName</a>
                                }
                                <span class="common-small-heading">
                                    <span class="">|</span>

