@model PurchaseOrderDetailViewModel

@{
    Layout = "~/Views/Shared/base.cshtml";
}


@section Title
{
    Order Details
}


@section ContentSection
{
    @Html.HiddenFor(m => m.OrderNumber)
    @Html.HiddenFor(m => m.Title)
    @Html.HiddenFor(m => m.AccountingCompanyId)
    @Html.HiddenFor(m => m.SelectedJustificationReason)
    @Html.HiddenFor(m => m.OrderId)
    @Html.HiddenFor(m => m.MessageDetailsJSON)
    @Html.HiddenFor(m => m.ActiveMobileTabClass)
