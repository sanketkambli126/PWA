                    <div class="tab-content">
                        <div class="clearfix"></div>
                        <div class="tab-grid">
                            <div class="col-md-12">
                                <div class="row">
                                    @Html.HiddenFor(m => m.InspectionUrl)
                                    @Html.HiddenFor(m => m.IsPscVisible)
                                    @Html.HiddenFor(m => m.IsOMVType)
                                    @Html.HiddenFor(m => m.IsCausesSectionVisible)
                                    @Html.HiddenFor(m => m.InspectionFindingFilter)
                                    @Html.HiddenFor(m => m.VesselId)
                                    @Html.HiddenFor(m => m.InspectionName)
                                    @Html.HiddenFor(m => m.OccuredDate)
                                    @Html.HiddenFor(m => m.VesselName)
                                    @Html.HiddenFor(m => m.IsAllSelected)
                                    @Html.HiddenFor(m => m.IsFindingOutstanding)
                                    @Html.HiddenFor(m => m.IsFindingOverdue)
                                    @Html.HiddenFor(m => m.IsPendingClosure)
                                    @Html.HiddenFor(m => m.IsClosed)
                                    @Html.HiddenFor(m => m.IsShowDetained)
                                    @Html.HiddenFor(m => m.IsDue)
                                    @Html.HiddenFor(m => m.IsOverdue)
                                    @Html.HiddenFor(m => m.strInspectionTypeIds)
                                    @Html.HiddenFor(m => m.InspectionFilter)
                                    @Html.HiddenFor(m => m.InDays)
                                    @Html.HiddenFor(m => m.IsInspection)
                                    @Html.HiddenFor(m => m.InspectionType)
                                    @Html.HiddenFor(m => m.MessageDetailsJSON)
