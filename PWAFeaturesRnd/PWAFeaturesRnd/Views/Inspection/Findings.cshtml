                                        <div class="">
                                            <span>Findings</span>
                                        </div>
                                    </div>
                                    <div class="data-grid-right">
                                        <div class="">
                                            <div class="radio-button-design">
                                                <div id="rbCrewStatus" rad>
                                                    <div class="custom-radio custom-control custom-control-inline">
                                                        <input class="custom-control-input" id="csall" name="selFindingStatus" type="radio" value="AllFindings" checked>
                                                        <label class="custom-control-label" for="csall">All (<span id="AllFindingLabelCount"></span>)</label>
                                                    </div>
                                                    <div class="custom-radio custom-control custom-control-inline">
                                                        <input class="custom-control-input" id="cscleared" name="selFindingStatus" type="radio" value="Cleared">
                                                        <label class="custom-control-label" for="cscleared">Cleared (<span id="ClearedLabelCount"></span>)</label>
                                                    </div>
                                                    <div class="custom-radio custom-control custom-control-inline">
                                                        <input class="custom-control-input" id="csoutstanding" name="selFindingStatus" type="radio" value="Outstanding">
                                                        <label class="custom-control-label" for="csoutstanding">Open (<span id="OutstandingLabelCount"></span>)</label>
                                                    </div>
                                                    <div class="custom-radio custom-control custom-control-inline">
                                                        <input class="custom-control-input" id="csover" name="selFindingStatus" type="radio" value="Overdue">
                                                        <label for="csover" class="custom-control-label">Overdue (<span id="OverdueLabelCount"></span>)</label>
