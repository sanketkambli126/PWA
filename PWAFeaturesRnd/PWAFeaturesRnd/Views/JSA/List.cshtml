                            <div class="counters-heading border-none mb-md-2">
                                <div id="">
                                    Status
                                </div>
                            </div>
                            <div class="clearfix jsatopmargin">
                                <div class="float-left mr-common">
                                    <div class="clearfix">
                                        <div class="float-left">
                                            <div class="dashboard-counters-label">
                                                Completed
                                            </div>
                                        </div>
                                        <div class="float-right">
                                            <a id="aCompletedNav" class="dashboard-counters" href="javascript:void(0)">
                                                <span id="completedCount" class="txt-blue">-</span>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <div class="float-left mr-common">
                                    <div class="clearfix">
                                        <div class="float-left">
                                            <div class="dashboard-counters-label">
                                                O/D for Closure
                                            </div>
                                        </div>
                                        <div class="float-right">
                                            <a id="aOverdueNav" class="dashboard-counters " href="javascript:void(0)">
                                                <span id="overdueClosureCount" class="txt-red">-</span>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <div class="float-left wp-mobile-100">
                                    <div class="clearfix">
                                        <div class="float-left">
                                            <div class="dashboard-counters-label">
                                                Pending Office Approval
                                            </div>
                                        </div>
                                        <div class="float-right">
                                            <a id="aPendOffAppNav" class="dashboard-counters" href="javascript:void(0)">
                                                <span id="approvalPendingCount" class="text-yellow">-</span>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-12 col-lg-12 col-xl-3 tab-box-1">
                        <div class="common-custom-card height-equal jsagraph">
                            <h1>Severity (Open)</h1>
                            <canvas id="jsagraph" height="80"></canvas>
                        </div>
                    </div>
                </div>
            </div>
            <div class="main-card card margin-bottom-custom table-common-design hide-mobile tab-box-2 top-box-margin" id="">
                <div class="common-custom-card mobile-pad-0 mb-0">
                    <div class="counters-heading border-none position-relative clearfix mb-0 float-none">
                        <div class="float-md-left">
                            <span>JSA Reports <span id="tableSubTitle"> </span> <span id="tblspndtrjsalist" class="table-title-date-filter"></span></span>
                        </div>
                        <div class="float-md-left filterdatadiv">
                            <a href="javascript:void(0)" data-toggle="modal" data-target="#filterdata" class="filter-design"><i class="fa fa-filter" aria-hidden="true" title=""></i><span id="appliedFilterCount">0</span> Filter(s) applied</a><a href="javascript:void(0)" class="clear-filter" id="aRemoveFilter"><i class="fa fa-fw" aria-hidden="true" title=""></i></a>
                        </div>
                    </div>
                    <div id="tableGrid" class="compact-table showing">
                        <table style="width: 100%;" id="dtjsalist" class="table table-hover table-striped table-bordered cardview row-sm">
                            <thead>
                                <tr>
                                    <th class="table-header-icon-align" data-toggle="tooltip" data-placement="top" title="Discussion"><img style="height:16px;" src="/images/message-black.svg" /></th>
                                    <th class="table-header-icon-align" data-toggle="tooltip" data-placement="top" title="Notes"><img style="height:15px;" src="/images/notes-black.svg" /></th>
                                    <th class="d-table-cell d-sm-none">Ref. No</th>
                                    <th class="d-table-cell d-sm-none">Title</th>
                                    <th>Chat/Notes</th>
                                    <th>Ref. No</th>
                                    <th>Title</th>
                                    <th>Start Date</th>
                                    <th>End Date</th>
                                    <th>Status</th>
                                    <th>Max. Risk</th>
                                    <th>System Area</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>

            <div class="loading-indicator d-none">
                <div class="loader bg-transparent no-shadow p-0">
                    <div class="ball-spin-fade-loader">
                        <div class="bg-teal"></div>
                        <div class="bg-teal"></div>
                        <div class="bg-teal"></div>
                        <div class="bg-teal"></div>
                        <div class="bg-teal"></div>
                        <div class="bg-teal"></div>
                        <div class="bg-teal"></div>
                        <div class="bg-teal"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>

