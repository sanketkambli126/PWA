@model PWAFeaturesRnd.ViewModels.JSA.JSAListViewModel
@{
    Layout = "~/Views/Shared/base.cshtml";
}

@section Title
{
    JSA - @Model.VesselName
}

@section ContentSection
{
    @Html.HiddenFor(m => m.EncryptedVesselId)
    @Html.HiddenFor(m => m.ActiveMobileTabClass)
    @Html.HiddenFor(m => m.StageName)
    @Html.HiddenFor(m => m.SelectedStatusIds)
    @Html.HiddenFor(m => m.SelectedSystemAreaIds)
    @Html.HiddenFor(m => m.SelectedRiskFilterIds)
    @Html.HiddenFor(m => m.OverdueForClosure)
    @Html.HiddenFor(m => m.GridSubTitle)
    @Html.HiddenFor(m => m.VesselName)

    <div class="app-main__outer fixed-page-header">
        <div class="app-main__inner background-padding new-background common-border-radius mobile-tab-bottom-padding">
            <div class="app-page-title header-shadow">
                <div class="page-title-wrapper">
                    <div class="margin-left-35">
                        <div class="">
                            <a class="common-heading-green"
                               href="@string.Concat(@Constants.DashboardVesselNavigation,@Model.EncryptedVesselId)">@Model.VesselName</a>
                        </div>
                        <div class="common-sub-heading-black">
                            Job Safety Analysis
                        </div>
                    </div>
                    <div class="page-title-actions">
                        <button class="btn btn-dark-grey text-white btn-shadow hover-blue mr-1 search-filter-sidebar d-md-inline-block d-none" id="divfilterhide">
                            <i class="fa fa-filter" aria-hidden="true" title=""><span class="open-sans-font bold-text ml-2 d-none d-md-inline-block">Filter</span></i>
                        </button>
                        @*d-md-inline-block*@
                        @*<div class=" d-none mr-1 mb-mobile-3 datepicker-mobile-design" id="divDatePickermobilehide">
                                <button class="btn btn btn-shadow btn-info" id="dtrjsalist" name="dtrjsalist">
                                    <i class="fa fa-calendar pr-1"></i>
                                    <span></span>
                                    <i class="fa pl-1 fa-caret-down"></i>
                                </button>
                            </div>*@
                        <button id="" data-toggle="tooltip" title="Export to excel" data-placement="bottom" class="btn btn-dark-grey text-white btn-export btn-shadow hover-blu  btnExport mobile-fixed-right tab-box-2 btnExportShowHideMobile">
                            <i class="fa fa-fw" aria-hidden="true"></i>
                        </button>
                    </div>
                </div>
            </div>
            <div class="content-pane">
                <div class="row " id="">
