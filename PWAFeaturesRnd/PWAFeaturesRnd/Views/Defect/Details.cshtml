@model DefectDetailsViewModel

@{
    Layout = "~/Views/Shared/base.cshtml";
}

@section Title
{
    Defect Details
}


@section ContentSection
{
    @Html.HiddenFor(m => m.EncryptedVesselId)
    @Html.HiddenFor(m => m.VesselName)
    @Html.HiddenFor(m => m.EncryptedDWOId)
    @Html.HiddenFor(m => m.DueDate)
    @Html.HiddenFor(m => m.IsCompletedOrClosed)
    @Html.HiddenFor(m => m.IsGuaranteeClaimCode)
    @Html.HiddenFor(m => m.MessageDetailsJSON)
    @Html.HiddenFor(m => m.IsStatusCompleted)
    @Html.HiddenFor(m => m.IsFromViewRecord)

    <div class="app-main__outer">
        <div class="app-main__inner background-padding new-background common-border-radius mobile-tab-bottom-padding padding-top-20">
            <div class="app-page-title header-shadow">
                <div class="page-title-wrapper d-lg-block d-xl-flex">
                    <div class="margin-left-35 width-55">
                        <div class="">
                            @if (Model.IsFromViewRecord == true)
                            {
                                <span class="common-small-heading">
                                    @Model.VesselName
                                </span>
                            }
                            else
                            {
                                <a class="common-heading-green"
                                   href="@string.Concat(@Constants.DashboardVesselNavigation,@Model.EncryptedVesselId)">@Model.VesselName</a>
                            }
                        </div>

                        <div class="common-sub-heading-black">
                            <span class="common-small-heading">
                                <span id="spanDefectName"> </span> |  <span id="spanDefectNumber"></span>
                                <span class="d-none d-md-inline-block">|</span>
                                <div>Due :  <span id="spanDefectDueDate"></span></div>
                            </span>

                        </div>
                    </div>

                    <div class="page-title-actions mobile-padding-0 left-fixed-ipad left-fixed-ipad-pro">
                        <partial name="DiscussionAndNotesCountButton" />
                        <button class="btn btn-dark-grey text-white btn-shadow hover-blue mr-1 hide-mobile btnDefectReport">
                            <img src="~/images/Download-doc-hover.png" class="m-0 align-top" /><span class="open-sans-font bold-text ml-2">Download Defect Report</span>
                        </button>
                        <button class="btn btn-dark-grey text-white btn-shadow hover-blue mr-1 hide-mobile btnDefectCloseAction btnDefectCloseActionDesktop">
                            @*data-toggle="modal" data-target="#closuredefect"*@
                            <img src="~/images/CloseDefectWhite.png" width="16" class="m-0 align-top" /><span class="open-sans-font bold-text ml-2">Close Defect</span>
                        </button>
                    </div>

                    <div class="dropdown mobile-dropdown-title d-block d-md-none d-lg-none d-xl-none">
                        <button type="button" id="mobileactiontoggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" class="btn btn-dark-grey text-white btn-export btn-shadow">
                            <span class="btn-icon-wrapper">
                                <i class="fa fa-ellipsis-v fa-w-6"></i>
                            </span>
                        </button>
                        <div tabindex="-1" role="menu" aria-hidden="true" class="dropdown-menu dropdown-menu-right" id="mobileActiondropdown">
                            <ul class="nav flex-column">
                                <li id="baseDiscussionMobile" class="d-none">
                                    <a class="nav-link discussionAnchorOnClick">
                                        <i class="fa fa-fw" aria-hidden="true" title=""></i><span>Discussions (<span class="baseDiscussionCountCommon">0</span>)</span>
                                    </a>
                                </li>
                                <li id="baseNotesMobile" class="d-none">
                                    <a class="nav-link noteAnchorOnClick">
                                        <i class="fa fa-fw" aria-hidden="true" title=""></i><span>My Notes (<span class="baseNotesCountCommon">0</span>)</span>
                                    </a>
                                </li>
                                <li>
                                    <a class="nav-link" id="">
                                        <img src="~/images/Download-doc-black.png" class="btnDefectReport" /><span> Download Defect Report</span>
                                    </a>
                                </li>
                                <li>
                                    <a class="nav-link btnDefectCloseAction btnDefectCloseActionMobile" id="">
                                        <img src="~/images/CloseDefectGray.png" /> <span>Close Defect</span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="content-pane">
                <div class="row">
                    <div class="col-12 col-md-12 col-lg-8 col-xl-8">
                        <div class="common-custom-card">
                            <div class="counters-heading border-none">
                                <div>
                                    <span>Description</span>
                                </div>
                            </div>
                            <p id="spanDefectDescription" class="open-sans-font details-common"></p>
                        </div>

                        <div class="common-custom-card">
                            <div class="counters-heading border-none">
                                <div class="clearfix">
                                    <div class="float-left">
                                        <div>
                                            <span class="mobilewidthrepair">Repair Specification</span>
                                        </div>
                                    </div>
                                    <div class="float-right float-mobile-left">
                                        <div class="download-attachments">
                                            <a class="txt-green-attachments btnDownloadAttachment cursor-pointer" data-target="#modalDownloadAttachment" data-toggle="modal">
                                                <img src="~/images/Download-doc-active.png" width="12" class="mr-1" /> <span>Download Attachment</span>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <p id="spanRepairSpecification" class="open-sans-font details-common"></p>
                        </div>

                        <div class="common-custom-card mobile-pad-0 table-common-design ">
                            <div class="counters-heading border-none mb-3 mb-md-2  position-relative">
                                <div>
                                    <span>Action Taken (<span id="spanActionTakenCount">0</span>)</span>
                                </div>
                            </div>
                            <div class="table-scroll-width">
                                <div class="table-responsive compact-table">
                                    <table style="width: 100%;" id="dtActionTaken" class="table table-hover table-striped table-bordered cardview row-sm table-fixed-layout">
                                        <thead>
                                            <tr>
                                                <th>Date</th>
                                                <th>Reported By</th>
                                                <th>Details</th>
                                            </tr>
                                        </thead>
                                    </table>
                                </div>
                            </div>
                        </div>

                        <div class="common-custom-card mobile-pad-0 table-common-design " id="divRequisition">
                            <div class="counters-heading border-none mb-3 mb-md-2  position-relative">
                                <div>
                                    <span>Requisition (<span id="spanRequisitionCount">0</span>)</span>
                                </div>
                            </div>
                            <div class="table-scroll-width">
                                <div class="table-responsive compact-table">
                                    <table style="width: 100%;" id="dtRequisition" class="table table-hover table-striped table-bordered cardview row-sm table-fixed-layout">
                                        <thead>
                                            <tr>
                                                <th>Order No.</th>
                                                <th>Priority</th>
                                                <th>Status</th>
