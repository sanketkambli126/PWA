@section ScriptSection
{
    <script src="~/assets/scripts/defectDetails.bundle.js"></script>
}

@section Modal
{
    <div class="modal fade" id="modalDownloadAttachment" role="dialog" aria-labelledby="modalDownloadAttachment" aria-hidden="true">
        <div class="modal-dialog" role="document" style="max-width:600px;">
            <div class="modal-content">
                <div class="modal-header header-background header-padding">
                    <div class="col-md-11 pl-0">
                        <div class="">
                            <a class="common-heading-green" id="vesselNameModal"></a>
                        </div>
                        <div class="common-sub-heading-black common-sub-heading-black-small">
                            Download Attachment
                        </div>
                    </div>
                    <div>
                        <button type="button" class="close pull-right p-0 mr-0 mt-0 font-size-close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">×</span>
                        </button>
                    </div>
                </div>
                <div class="modal-body new-background padding-20">
                    <div class="table-common-design icon-padding">
                        <div class="common-custom-card mb-0 mobile-pad-0">
                            <div class="counters-heading border-none">
                                <div class="">
                                    <span>Attachments</span>
                                </div>
                            </div>
                            <div class="table-responsive compact-table">
                                <table style="width: 100%;" id="dtDownloadAttachment" class="table table-hover table-striped table-bordered cardview row-sm">
                                    <thead>
                                        <tr>
                                            <th></th>
                                            <th>Title</th>
                                            <th>Type</th>
                                            <th>Created Date</th>
                                        </tr>
                                    </thead>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

