@model VoyageReportingRequestViewModel

@{
    Layout = "~/Views/Shared/base.cshtml";
}

@section Title
{
    Sea Passage Event - @Model.VesselName
}


@section ContentSection
{

    @Html.HiddenFor(m => m.ListURL)
    @Html.HiddenFor(m => m.EncryptedVesselDetail)
    @Html.HiddenFor(m => m.PositionListId)
    @Html.HiddenFor(m => m.MessageDetailsJSON)
    @Html.HiddenFor(m => m.ActiveMobileTabClass)
    @Html.HiddenFor(m => m.IsFromViewRecord)

    <div class="app-main__outer fixed-page-header">
        <div class="app-main__inner background-padding new-background common-border-radius mobile-tab-bottom-padding">
            <div class="app-page-title header-shadow">
                <div class="page-title-wrapper">
                    <div class="margin-left-35 width-70">
                        <div class="">
                            @if (Model.IsFromViewRecord == true)
                            {
                                <span class="common-small-heading">
                                    @Model.VesselName
                                </span>
                            }
                            else
                            {
                                <a class="common-heading-green"
                                   href="@string.Concat(@Constants.DashboardVesselNavigation,@Model.EncryptedVesselDetail)"> @Model.VesselName</a>
                            }

                        </div>
                        <div class="common-sub-heading-black pr-0">
                            <span id="spanActivityName">-</span>,
                            <div class="d-block d-md-inline-block">
                                <span id="spanFromDestination">-</span> to <span id="spanToDestination">-</span>
                            </div>
                        </div>
                    </div>
                    <div class="page-title-actions mt-0 mt-lg-0 mobile-padding-0 left-fixed-ipad">
                        <partial name="DiscussionAndNotesCountButton" />
                    </div>
                    <div class="dropdown mobile-dropdown-title d-block d-md-none d-lg-none d-xl-none">
