    var noonReportVesselDraft = Model.NoonReportVesselDraft;
    var noonPowerOutput = Model.NoonReportSlipAndPowerOutput;
    var freshWaterDomestic = Model.FreshWaterDomestic;
    var freshWaterTechnical = Model.FreshWaterTechnical;
    var LubeOilClo = Model.LubeOilClo;
    var LubeOilCrankCase = Model.LubeOilCrankCase;
    var LubeOilAux = Model.LubeOilAux;
    var LubeOilGeneral = Model.LubeOilGeneral;
    var WasterRobSludge = Model.WasterRobSludge;
    var WasterRobBilge = Model.WasterRobBilge;
    var WasterRobSlops = Model.WasterRobSlops;
    var WasterRobSewage = Model.WasterRobSewage;
}


<div class="modal-header header-background header-padding modaldetails-design">
    <div class="col-md-11 pl-0 pr-0">
        <div class="common-sub-heading-black noonheadershowmorewrapper">
            <span class="header">
                <div class="common-small-heading" id="divHeaderNoon">-</div>
            </span>
            <div class="common-small-heading">
                <span class="common-heading-green"><div><span id="">@Model.VesselName</span></div></span>
                @if (!string.IsNullOrEmpty(Model.Date) && Model.Date != "-")
                {
                    <span class="d-none d-md-inline-block dividerline">|</span>
                    <span class="common-small-heading"><span class="head">Date  </span> <span id="" class="value">@Model.Date</span></span>
                }

                @if (!string.IsNullOrEmpty(Model.SelectedStartUTC) && Model.SelectedStartUTC != "-")
                {
                    <span class="d-none d-md-inline-block dividerline">|</span>
                    <span class="common-small-heading"><span class="head">+/- UTC  </span> <span id="" class="value">@Model.SelectedStartUTC</span></span>
                }

                @if (NoonReportNvaigation.IsNoon && Model.CanShowIDL)
                {
                    @if (!string.IsNullOrEmpty(Model.CrossedIDLDays) && Model.CrossedIDLDays != "-")
                    {
                        <span class="d-none d-md-inline-block dividerline">|</span>
                        <span class="common-small-heading">
                            <span class="head">Crossed IDL  </span> <span id="" class="value">@Model.CrossedIDLDays</span>
                        </span>
                    }
                }

                @if (!string.IsNullOrEmpty(Model.BusinessTime) && Model.BusinessTime != "-")
                {
                    <span class="d-none d-md-inline-block dividerline">|</span>
                    <span class="common-small-heading">
                        <span class="head">Buisness Time  </span> <span id="" class="value">@Model.BusinessTime</span>
                    </span>
                }

                @if (!string.IsNullOrEmpty(Model.NewShipTime) && Model.NewShipTime != "-")
                {
                    <span class="d-none d-md-inline-block dividerline">|</span>
                    <span class="common-small-heading">
                        <span class="head">Ship Clock (hrs)  </span> <span id="" class="value">@Model.NewShipTime</span>
                    </span>
                }
                @if (!string.IsNullOrEmpty(Model.SecurityLevel) && Model.SecurityLevel != "-")
                {
                    <span class="d-none d-md-inline-block dividerline">|</span>
                    <span class="common-small-heading">
                        <span class="head">Security Level  </span> <span id="" class="value">@Model.SecurityLevel</span>
                    </span>
                }

                @if (Model.SpaRouteChangedYes != null)
                {
                    <span class="d-none d-md-inline-block dividerline">|</span>
                    <div class="common-small-heading">
                        <span class="head">
                            Has your planned route   &nbsp;<span class="common-sub-heading-red">changed due to weather? </span>
                        </span>
                        <span id="">
                            @if (Model.SpaRouteChangedYes == true)
                            {
                                <span class="value">Yes</span>
                            }
                            else if (Model.SpaRouteChangedYes == false)
                            {
                                <span class="value">No</span>
                            }
                        </span>
                    </div>
                    @if (Model.SpaRouteChangedYes == true)
                    {
                        <span class="d-none d-md-inline-block dividerline">|</span>
                        <span class="common-small-heading">
                            <span class="head">Reason  </span> <span id="" class="value"> @Model.PlkIdRouteChangeReason</span>
                        </span>
                    }
                }


            </div>
        </div>
    </div>
    <div>
        <button type="button" class="close pull-right p-0 mr-0 mt-0 font-size-close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">×</span>
        </button>
      
        @if (Model.IsNoonIncomplete == true)
        {
                <br/><br/>
                <span class="dashboard-counters-label text-danger">Draft</span>
        }
    </div>
</div>
