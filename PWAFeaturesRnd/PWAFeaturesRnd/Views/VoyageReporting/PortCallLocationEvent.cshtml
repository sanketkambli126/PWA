    <div class="modal fade page-popup-design-2 allvesselmodal" id="modalEventsDocument" tabindex="-1" role="dialog" aria-labelledby="modalEventsDocument" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered w-850 modal-margin" role="document" style="box-shadow:none;">
            <div class="modal-content">
                <div class="modal-header p-0">
                    <div class="common-custom-card mb-0 w-100 pb-0">
                        <div class="counters-heading border-none">
                            <div>
                                <span id="agentdetails">Port / Call Documents</span>
                            </div>
                        </div>
                        <div class="subtitle-heading mr-0">
                            <label>Event Name</label>
                            <span id="spanEventsName" class="align-middle vessel-default-color"></span>
                            <label class="hide-mobile mr-0">|</label>
                        </div>
                        <div class="subtitle-heading mr-0">
                            <label class="ml-0 ml-sm-1">From Date</label>
                            <span id="spanFromDate" class="align-middle vessel-default-color"></span>
                            <label class="hide-mobile mr-0">|</label>

                        </div>
                        <div class="subtitle-heading mr-0">
                            <label class="ml-0 ml-sm-1">To</label>
                            <span id="spanToDate" class="align-middle vessel-default-color"></span>
                        </div>
                    </div>
